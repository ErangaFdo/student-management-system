<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="AnchorPaneLogin" prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.gdse.studentmanagementsystem.Controller.LoginFormController">
   <children>
      <TextField fx:id="textUserName" layoutX="299.0" layoutY="245.0" prefHeight="34.0" prefWidth="314.0" promptText="enter your name" />
      <TextField fx:id="TextUserPassword" layoutX="299.0" layoutY="302.0" prefHeight="34.0" prefWidth="314.0" promptText="enter your password" />
      <Button fx:id="btnLogin" layoutX="406.0" layoutY="363.0" mnemonicParsing="false" onAction="#btnLoginOnAction" prefHeight="34.0" prefWidth="99.0" text="Login">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
